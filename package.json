  "author": "David Orr <davidyorr@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "copy-assets": "copyfiles --flat src/assets/**/* dist/assets",
    "build": "pnpm tsc --extendedDiagnostics",
    "postbuild": "pnpm run copy-assets",
    "prestorybook": "pnpm run copy-assets",
    "prestorybook:build": "pnpm run copy-assets",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --output-dir ./public/storybook",
    "format": "prettier --write src stories .storybook cypress",
    "lint": "eslint --ext .ts,.tsx src stories .storybook cypress",
    "cypress": "cypress run"
  },
  "devDependencies": {
    "@storybook/addon-backgrounds": "7.4.0",
    "@storybook/addon-controls": "7.4.0",
    "@storybook/client-api": "7.4.0",
    "@storybook/react": "7.4.0",
    "@storybook/react-vite": "7.4.0",
    "@types/css-font-loading-module": "0.0.4",
    "@types/cypress-image-snapshot": "3.1.6",
    "@types/node": "14.11.4",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "axios-mock-adapter": "1.20.0",
    "chart.js": "2.9.4",
    "concurrently": "7.0.0",
    "copyfiles": "2.4.1",
    "cypress": "9.2.1",
    "cypress-image-snapshot": "4.0.1",
    "cypress-wait-until": "1.7.2",
    "eslint": "7.11.0",
    "eslint-plugin-storybook": "0.6.13",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "storybook": "7.4.0",
    "typescript": "5.2.2",
    "vite": "4.4.9"
  },
  "dependencies": {
    "@types/chart.js": "^2.9.24",
    "axios": "^0.24.0",
    "axios-cache-adapter": "^2.7.3",
    "canvas": "^2.8.0",
    "comma-number": "^2.0.1",
    "parse-ms": "^2.0.1"
  },
  "peerDependencies": {
    "chart.js": "^2.9.4"
  }
